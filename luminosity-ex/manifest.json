{
  "manifest_version": 2,
  "name": "Luminosity Bookmark",
  "description": "Luminosity Bookmark Chrome Extension",
  "version": "0.1",
  "icons": {
    "128": "luminosity-logo.png"
  },
  "browser_action": {
    "default_icon": "luminosity-logo.png",
    "default_popup": "credentials.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "identity",
    "bookmarks"
  ],
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "oauth2": {
    "client_id": "314360845682-qk6r8nksmduncf8rvfciu9d6f672i52q.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": ""
}
